@using AirlineReservation.Models;
@model Adres
@{
    ViewData["Title"] = "Create";
}
<div class="container">

<form asp-action="create">
    <div class="form-group">
        <label asp-for="Address">Address</label>
        <input asp-for="Address" class="form-control" placeholder="Enter Adress">
    </div>
    <div class="form-group">
        <label asp-for="Phone">Phone</label>
        <input asp-for="Phone" class="form-control" placeholder="Enter Phone">
    </div>
    <div class="form-group">
        <label asp-for="AdminMail">Email address</label>
        <input type="email" asp-for="AdminMail" class="form-control" placeholder="Enter email">
    </div>
        <div style="display:flex;justify-content:center">
            <button type="submit" class="btn btn-primary">Submit</button>
        </div>
</form>
</div>