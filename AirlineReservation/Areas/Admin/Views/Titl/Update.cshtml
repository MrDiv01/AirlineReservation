@using AirlineReservation.Models;
@model Titile
@{
    ViewData["Title"] = "Update";
}

<div class="container">

    <form method="post" asp-action="Update" enctype="multipart/form-data">
        <div class="form-group">
            <label asp-for="Name">Decription</label>
            <input asp-for="Name" class="form-control" placeholder="Description">
        </div>
        <div class="form-group">
            <label asp-for="ImageFile">Images</label>
            <input asp-for="ImageFile" class="form-control" placeholder="Image">
        </div>
        <span asp-validation-for="ImageFile" class="text-danger"></span>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>